{
  "name": "q-admin-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "polyfill": "node ./scripts/fix_keep_alive.cjs",
    "dev": "pnpm run polyfill && vite --force",
    "build": "pnpm run polyfill && run-p type-check \"build-only {@}\" --",
    "preview": "pnpm run polyfill && pnpm run build && vite preview",
    "build-only": "pnpm run polyfill && vite build",
    "type-check": "vue-tsc --build --force",
    "format": "prettier --write src/",
    "lint": "npx oxlint .",
    "lint:fix": "npx oxlint --fix ."
  },
  "dependencies": {
    "axios": "^1.7.4",
    "element-plus": "^2.8.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.1",
    "plus-pro-components": "^0.1.14",
    "qs": "^6.13.0",
    "rxjs": "^7.8.1",
    "utils94": "^2.2.4",
    "vue": "^3.4.37",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@iconify-json/material-symbols": "^1.1.87",
    "@plus-pro-components/resolver": "^0.0.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.3.0",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@unocss/preset-icons": "^0.62.1",
    "@unocss/preset-uno": "^0.62.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/tsconfig": "^0.5.1",
    "npm-run-all2": "^6.2.2",
    "oxlint": "^0.7.1",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "typescript": "~5.5.4",
    "unocss": "^0.62.1",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.0",
    "vite-plugin-oxlint": "^1.0.5",
    "vue-tsc": "^2.0.29"
  },
  "license": "MIT"
}