{
  "name": "q-admin-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "polyfill": "node ./scripts/fix_keep_alive.cjs",
    "dev": "pnpm run polyfill && vite --force",
    "build": "pnpm run polyfill && run-p type-check \"build-only {@}\" --",
    "preview": "pnpm run polyfill && pnpm run build && vite preview",
    "build-only": "pnpm run polyfill && vite build",
    "type-check": "vue-tsc --build --force",
    "format": "prettier --write src/",
    "lint": "npx oxlint .",
    "lint:fix": "npx oxlint --fix ."
  },
  "dependencies": {
    "axios": "^1.7.4",
    "element-plus": "^2.8.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "plus-pro-components": "^0.1.21",
    "qs": "^6.14.0",
    "rxjs": "^7.8.2",
    "ts-md5": "^1.3.1",
    "utils94": "^2.2.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@iconify-json/material-symbols": "^1.2.17",
    "@plus-pro-components/resolver": "^0.0.3",
    "@tsconfig/node20": "^20.1.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.14",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.18",
    "@unocss/preset-icons": "^66.0.0",
    "@unocss/preset-uno": "^66.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/tsconfig": "^0.7.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.3",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "typescript": "~5.8.2",
    "unocss": "^66.0.0",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.3",
    "vite-plugin-oxlint": "^1.3.0",
    "vue-tsc": "^2.2.8"
  },
  "license": "MIT"
